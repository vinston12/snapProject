<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.snapchatcopy"
    tools:ignore="GoogleAppIndexingWarning">

    <application
        android:icon="@mipmap/snapchat512"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/snapchat512"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    <activity android:name=".ActivitySignIn"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.NoActionBar">
    </activity>

    <activity
            android:name=".ActivitySingUp"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            >
    </activity>
        <activity
            android:name=".AfterLoginActivity.CameraActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            >
        </activity>
        <activity
            android:name=".AfterLoginActivity.ChatActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            >
        </activity>
        <activity
            android:name=".AfterLoginActivity.StoryActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            >
        </activity>
</application>

</manifest>